{
  "require": {
    "craftcms/ckeditor": "1.0.0-beta.2",
    "craftcms/cms": "3.3.4.1",
    "craftcms/redactor": "2.4.0",
    "spicyweb/craft-embedded-assets": "2.0.12",
    "vlucas/phpdotenv": "^2.4.0",
    "ext-imagick": "*"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
